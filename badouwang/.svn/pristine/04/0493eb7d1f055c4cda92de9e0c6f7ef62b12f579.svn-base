.p---------------------p{}
.pwid .tising{float:right;width:285px;margin-top:10px;}
.pwid .tising .tisle{margin:12px 10px 12px 0;border-left:4px solid #009170;font-size:16px; font-family:"微软雅黑";text-indent:10px;}
.pwid .tising .tisbor{border-top:1px solid #ddd;}
.pwid .tising .tisgao{padding:15px 20px;}
.pwid .tising .tisgao .bot{padding:5px 0;color:#555;width:245px;height:16px;overflow:hidden;}
.pwid .tising .tisgao .tom{padding:5px 0;color:#f03;width:245px;height:16px;overflow:hidden;}


.pwid .tising .tisero{ width:245px; height:60px;  margin-left:30px; margin-bottom:30px;}
.pwid .tising .tisero .img{ width:60px; height:60px; float:left;}
.pwid .tising .tisero .yeo{ float:right; width:170px;}
.pwid .tising .tisero  .yeo a{ font-size:14px; font-weight:bold; color:#646464; margin-right:10px;}
.pwid .tising .tisero  .yeo a:hover{ color:#ff7100;}
.pwid .tising .tisero  .yeo p{ color:#969696; font-size:14px; line-height:35px;}