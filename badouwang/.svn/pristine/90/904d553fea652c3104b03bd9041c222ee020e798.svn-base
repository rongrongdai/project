@charset "utf-8";
/* CSS Document */
/*textarea{resize: none;}*/
.Conten-tone{ width:200px; line-height:60px; background-color:#00b38a; font-weight:bold; text-align:center; font-size:14px; font-family:'Microsoft YaHei';  }
.Coner{width:1200px;margin:0 auto;}
.Cow-left{ width:200px; border:1px #009071 solid; float:left;border-radius:4px 4px 0 0;}
.Cow-left .Coner-con{margin:5px 0;display:none;}
.Cow-left .Con-tit{font-family:'Microsoft YaHei';line-height:34px;font-size:15px; color:#00b38a;margin:0 0 1px 0;text-align:center; cursor:pointer;}
.Cow-left .Con-tit .ma{border-top:1px solid #00b38a;}
.Cow-left .Con-tit .mar{border-bottom:1px solid #00b38a;border-top:1px solid #FFF;background:#00b38a;box-shadow: 0px 2px 3px #ccc;color:#FFF;}
.Cow-left .Coner-con a{font-family:'Microsoft YaHei';display:block; text-indent:60px;line-height:28px;color:#555;width:80%;margin:1px auto;font-size:14px; text-decoration:none;}
.Cow-left .Coner-con a:hover{background:#B4EDDF;color:#222;}
.Cow-left .Coner-con .aclick{background:#B4EDDF;color:#222;}
.Cow-left dd a{ width:145px; font-family:SimSun; font-size:12px; color:#666; text-decoration:none; line-height:26px;}
.Cow-left dd hr{ width:145px;  position:relative; left:-25px; }
.Cow-left dd a:hover { text-decoration:none; color:#c00;}
.Con-ifr-right{ width:996px;overflow:auto; float:left;padding:12px 0 0 0;}
/*个人设置*/
.persoall{ }
.per{width:955px; border:1px #00b38a solid; border-radius:4px;}
.per-top{ height:50px; border-bottom:1px solid #00b38a;}
.per-top img{ float:left; margin:5px 16px 0px 16px;}
.per-top .set-up{float:left;margin:0;padding:16px 0; font-family:'Microsoft YaHei';line-height:18px;color:#00b38a;}


.per-form{padding:0 0 10px 60px;}
.per-form .pheig{padding:10px 0;height:30px;line-height:30px;}
.per-form .pheigtet{padding:10px 0;height:80px;line-height:30px;}

.per-form .pheigtet textarea{height:60px;width:400px;line-height:22px;padding:5px;border:1px solid #00b38a;resize:none;}

.per-form .pheig .inputcs{height:30px;line-height:22px;padding:0 5px;border:1px solid #00b38a;}
.per-form .pheig select{height:30px;line-height:22px;border:1px solid #00b38a;}
.per-form .pflat1{float:left;width:95px;font-family:'Microsoft YaHei';font-size:14px;color:#00b38a;}
.per-form .pflat2{float:left;font-family:'Microsoft YaHei';font-size:14px;}
.per-form .pflat2 #chtt,#chtc{margin:0 0 0 5px;color:#f90;cursor:pointer;}
.per-form .prompt{color:#999;font-size:14px;}
.per-form .pro-p{color:#999;font-size:14px;}
.per-form .pflat2pb input{float:left;}
.per-form .pflat2pb span{float:left;display:block;line-height:34px;margin:0 12px 0 2px;}
.per-form .pflat2tx input{padding:4px 0 0 0;}
.per-form .psub{width:500px;margin:0 auto; clear:both;}

.per-form .psub .subinput{font-family:'Microsoft YaHei';font-size:13px;color:#FFF; cursor:pointer;background:none; width:100px;height:40px; box-shadow:inset 0 25px 30px #00b38a;}


.per-form .psub .subinput:hover{box-shadow:inset 0 10px 30px #00b38a;color:#00b38a}


.per .per-make{height:40px;border-bottom:1px solid #9DE1BD;padding:5px 0 0 30px; position:relative;}
.per .per-make .make{font-family:'Microsoft YaHei';float:left;width:80px;border-radius:4px 4px 0 0;border:1px solid #9DE1BD; text-align:center; line-height:39px;height:39px; cursor:pointer; background:aliceblue;}
.per .per-make .aclick{background:#FFF;border-bottom:none;position:relative;height:40px;line-height:40px;color:#00b38a;}
.per .per-make .make:hover{color:#00b38a;}


/*已预约*/
#hiddyi{ }
.per .padd,.per .padd1{ padding:20px;overflow:hidden;}
.per .padd td,.per .padd1 td{font-family:'Microsoft YaHei'; font-size:14px; color: #555; text-align:center; border:1px #9DE1BD  solid;height:40px;line-height:40px; }
.per .padd .td-title,.per .padd1 .td-title{ text-align:center; color:#000; background:#E7F5EC; }
.per .padd .ts-tit,.per .padd1 .ts-tit{font-family:'Microsoft YaHei';width:300px;height:30px;line-height:30px;float:left;}
.per .padd .ts-but,.per .padd1 .ts-but{font-family:'Microsoft YaHei';width:300px;height:30px;line-height:30px;float:left;}
td p{font-size: 14px;}


/*基本信息*/

	.per-in{ position:relative;height:600px;border-radius:4px 4px 0 0;border:1px solid #00b38a;box-shadow: 0px 2px 3px #00b38a;margin:0 0 8px 0;}
    .per-in .per-inimg{ position:absolute;width:100px;height:100px;top:55px;left:20px;background:#F93;border:1px solid #CCC;}

	.per-inimghy{ width:102px; text-align:center; color:#FFF; background:#00b38a; margin:0px 0px 0px -1px; cursor:pointer;}

    .per-in .per-intop{height:35px;background:#00b38a;box-shadow: 0px 2px 3px #ccc; color:#FFF; padding:10px 0px 0px 20px; font-size:20px; font-weight:700;}
	
	
	
    .per-in .per-incon{ padding:0 0 0 140px;}
    .per-in .per-incon .incon-sys{font-size:18px;font-family:'Microsoft YaHei';line-height:46px;}
	
	.per-in .per-incon .incon-sysry{ color:#f67458; font-size:12px; margin-left:20px;}
    .per-in .per-incon .incon-bal{line-height:30px;overflow:hidden;}
    .per-in .per-incon .incon-bal .bal{float:left;font-family:'Microsoft YaHei';font-size:14px;margin:0 20px 0 0;}
    .per-in .per-incon .incon-bal .bal span{padding:0 3px 0 0;color:#F93;}
    .per-in .per-incon .incon-bal .bal-gy{float:left;border-radius:20px;width:70px; text-align:center;color:#FFF; cursor:pointer;}
    .per-in .per-incon .incon-bal .bal-cz{background:#6E8B00;margin:0 0 0 100px;}
    .per-in .per-incon .incon-bal .bal-tx{background:#00b38a;margin:0 0 0 20px;}
    .per-in .per-incon .incon-bal .bal-cz:hover{background:#F93917;}
    .per-in .per-incon .incon-bal .bal-tx:hover{background:#6E0000;}
    .per-in .per-incon .incon-inf{ width:520px; margin-top:50px;}
	.incon-infy{ width:110px; height:110px; background:#00b38a; float:left; margin:20px 40px 0px 0px; color:#FFF; font-size:18px;}
    .incon-infy p,.incon-infy img{ margin:10px 0px 0px 30px;}
/*基本信息结束*/	




/*个人资料*/

