.hy-indall .hy-inone{ width:900px; height:196px; border:1px #C0CFCB solid; margin-bottom:20px; }
.hy-indall .hy-inone .hy-iwy{ padding:18px; float:left;}
.hy-indall .hy-inone .hy-iwy .hy-img{ width:150px; height:150px;}
