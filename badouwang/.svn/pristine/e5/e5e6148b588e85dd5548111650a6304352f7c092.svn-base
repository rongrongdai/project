@charset "utf-8";
/* CSS Document */

body,p,blockquote,form {margin:0; padding:0;}



/*培训主页ALL*/
#playBox{ width:100%; height:400px; background:#333; position:relative; overflow:hidden;}
#playBox .oUlplay { width:99999px; position:absolute; left:0; top:0;}
#playBox .oUlplay li{ float:left;}
#playBox .pre{ cursor:pointer; width:45px; height:45px; background:url(../../images/home/pxu/l.png) no-repeat; position:absolute; top:190px; left:10px; z-index:2;}
#playBox .next{ cursor:pointer; width:45px; height:45px; background:url(../../images/home/pxu/r.png)no-repeat; position:absolute; top:190px; right:10px; z-index:2;}
#playBox .smalltitle {height:10px; position:absolute; bottom:15px; z-index:2}
/*#playBox .smalltitle ul{ width:120px;  margin-left:730px;}*/
#playBox .smalltitle ul li{ width:10px; height:10px; margin:0 5px; border-radius:10px; background:#ffffff; float:left; overflow:hidden;*display:inline;*zoom:1; cursor:pointer;}
#playBox .smalltitle .thistitle{ background:#69aaec;}
#playBox ul{list-style-type: none;padding:0;margin:0;}
#playBox li{list-style-type: none;}


.con_a{width:1200px;margin:65px auto 10px auto; overflow:hidden; position:relative}
.con_a .con{width:280px;height:200px; overflow:hidden;float:left;margin-right:24px;border:1px solid #eee;}
.con_a .con h2{ text-align:center;color:#333;margin:10px 0;}
.con_a .con .tet{height:150px; padding:0 20px;line-height:30px;text-indent:28px;color:#888;}
.con_a .con .inp{height:35px;width:125px;margin:8px auto;box-shadow:3px 3px 10px #999;}
.con_a .con .inp .test{display:block;height:35px;line-height:35px; text-decoration:none; text-align:center;width:90px;float:left;background:#D85B01;color:#FFF;border-radius:3px 0 0 3px ;transition:background ease .6s}
.con_a .con .inp span{float:left;width:35px;height:35px; background:#EC6000;border-radius:0 3px 3px 0 ; position:relative;transition:background ease .6s }
.con_a .con .inp span a{border-color:transparent transparent transparent #FFF ;border-style: solid;border-width: 9px 9px 9px 9px;width: 0;height: 0;position: absolute;top: 9px;left:15px;}
.con_a .con .inp:hover .test{background:#009170;}
.con_a .con .inp:hover span{background:#009000;}
.con_a .con .title{overflow:hidden;height:44px;border-right:1px solid #eee;}
.con_a .con .title img{float:left; background:#EEE;}
.con_a .con .title div{float:left;width:178px;height:39px;line-height:39px; text-indent:30px;background:#EEE no-repeat;}
.con_a .con .link{margin:0 0 0 4px;border:1px solid #eee;border-top:none;height:155px;}
.con_a .con .link a{display:block;	height:25px;line-height:25px;font-size:12px;text-indent:10px;text-decoration:none;color:#555;}
.con_a .con .link a:hover{background:#ddd;color:#009170;}
.con_a .con .link a div{width:190px;float:left;}
.con_a .con .link a span{float:left;font-size:12px;}



