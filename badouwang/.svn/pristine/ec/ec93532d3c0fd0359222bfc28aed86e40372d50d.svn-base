.any-ysnew{ width:840px; border-bottom:1px #CCC dashed; padding:10px 0;overflow:hidden;}
.any-ysnew .any-gyd{ color:#313131; font-size:18px; font-weight:normal; line-height:1.5; word-break:break-all; word-wrap:break-word; white-space:normal; cursor:pointer;  width:840px; overflow:hidden; }
.any-ysnew .any-gyd a{ color:#313131;}
.any-ysnew .any-gyd a:hover{color: #ea5a3d;}
.any-ysnew .any-gyd .span{ padding:3px; background:#00b38a; color:#FFF; border-radius:5px; font-weight:bolder; margin-right:5px;}
.any-ysnew .any-gyd .imgs{ width:23px; height:20px;}
.any-ysnew .any-gyd .spany{color:#d8110b; font-size:14px;}

.any-ysnew .any-ger{ width:840px; overflow:hidden; margin-top:20px;}
.any-ysnew .any-ger p{color:#808080; line-height:1.6; font-size:14px;}
.any-ysnew .any-ger a{ color:#00b38a;}
.any-ysnew .any-ger a:hover{text-decoration: underline;}

.any-ysnew .any-gsan{width:840px; overflow:hidden; color:#808080; font-size:14px; margin:20px 0px;}
.any-ysnew .any-gsan img{ width:25px; height:25px;}
.any-ysnew .any-gsan  .sper{ margin:0px 10px;}
.any-ysnew .any-gsan  .spnd{color:#d8110b;}





.anYe-cen-right{ width:300px; float:left; margin-left:60px; margin-top:20px;}
